<launch>
  
  <node pkg="pandora_exploration" type="exploration_controller" name="explorer" output="screen" respawn="true" >
    <rosparam file="$(find pandora_exploration)/config/explore_costmap_params.yaml" command="load" ns="explore_costmap" />
    <rosparam file="$(find pandora_exploration)/config/coverage_costmap_params.yaml" command="load" ns="coverage_costmap" />
    <rosparam file="$(find pandora_exploration)/config/frontier_exploration_params.yaml" command="load" />
    <rosparam file="$(find pandora_exploration)/config/exploration_controller_params.yaml" command="load" />
  </node>

</launch>
